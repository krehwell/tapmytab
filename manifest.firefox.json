{
    "manifest_version": 3,
    "name": "tapmytab",
    "version": "1.5.2",
    "action": {
        "default_icon": {
            "16": "favicon-16x16.png",
            "32": "favicon-32x32.png",
            "192": "android-chrome-192x192.png"
        }
    },
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png",
        "192": "android-chrome-192x192.png",
        "512": "android-chrome-512x512.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "tapmytab@krehwell.com",
            "strict_min_version": "109.0"
        }
    }
}
